-- Rename the existing products table
ALTER TABLE products RENAME TO products_old;

-- Create the new products table with the additional columns
CREATE TABLE products (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    description TEXT,
    price REAL NOT NULL,
    stock INTEGER NOT NULL,
    image_path TEXT,
    category_id INTEGER,
    store_id INTEGER,
    FOREIGN KEY (category_id) REFERENCES categories(id),
    FOREIGN KEY (store_id) REFERENCES stores(id)
);

-- Copy data from the old products table to the new products table
INSERT INTO products (id, title, description, price, stock, image_path)
SELECT id, title, description, price, stock, image_path
FROM products_old;

-- Drop the old products table
DROP TABLE products_old;

PRAGMA foreign_keys=on;</sql><current_tab id="0"/></tab_sql></sqlb_project>
